<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>^com\.hp\.pkg\.swls\.printer-essentials-[a-z]+\.version$</key>
	<dict>
		<key>Type</key>
		<string>ESSENTIAL-REQUIRED</string>
		<key>Icon</key>
		<string>essential</string>
	</dict>
	<key>^com\.apple\.pkg\.HPScan[0-9]*$</key>
	<dict>
		<key>Type</key>
		<string>ESSENTIAL-OPTIONAL</string>
		<key>Icon</key>
		<string>generic_software</string>
	</dict>
</dict>
</plist>
